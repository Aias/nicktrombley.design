{
	"useTabs": true,
	"tabWidth": 2,
	"singleQuote": true,
	"trailingComma": "es5",
	"jsxBracketSameLine": true,
	"jsxSingleQuote": true,
	"quoteProps": "consistent",
	"semi": true,
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
